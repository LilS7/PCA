    "def four_labels(type_):\n",
    "#     analysts = ['INTJ', 'INTP', 'ENTJ', 'ENTP']\n",
    "#     diplomats = ['INFJ', 'INFP', 'ENFJ', 'ENFP']\n",
    "#     sentinels = ['ISTJ', 'ISFJ', 'ESTJ', 'ESFJ']\n",
